  <Code>Public Dim Page, Pages As Integer
Private Dim Line, Rows, Offset, Range As Integer

Public Function Init(ByVal Rows, ByVal Offset, ByVal Range) As Boolean
    Me.Pages = (Rows + Offset) \ Range
    Me.Line = Offset
    Me.Rows = Rows
    Me.Offset = Offset
    Me.Range = Range
End Function

Public Function IsHeaderHidden()
    IsHeaderHidden = (Me.Line &lt;&gt; Me.Offset) And ((Me.Line Mod Me.Range) &lt;&gt; 0)
    If Not IsHeaderHidden Then Me.Page += 1
End Function

Public Function IsFooterHidden()
    IsFooterHidden = (Me.Line Mod Me.Range) &lt;&gt; 0
End Function

Public Function IsLineHidden(ByVal Line)
    IsLineHidden = IsNothing(Line)
    If Not IsLineHidden Then Me.Line += 1
End Function</Code>
